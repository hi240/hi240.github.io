import java.util.Scanner;
public class hi {
	public static void main(String[] args) {
		//별찍기
				for(int i =2; i<=9; i++) {
					for (int j=1;j<=9;i++) {
						if(i==j) {
							continue; //아래 구문을 스킵하세요.
						}
						System.out.printf("%s","*");
					}
					System.out.println();
				}
			
				for(int i=2;i<=9;i++) {
					for(int j=1;j<=9;j++) {
						System.out.println("*");
					}
				System.out.println();
				}
				
	for(int i=2;i<=9;i++) {
		for(int j=1;j<i;j++) {
			System.out.print("*");
		}
		System.out.println();
	}
	}
}

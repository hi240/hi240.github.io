package kr.or.bit;

public class TestAir {
	public static void main(String[] args) {
				
		AirPlane air= new AirPlane();
		air.name="KOREAN";
		air.number=102;
		air.airInfo(); 
		AirPlane air2= new AirPlane();
		air2.name="ASIANA";
		air.number=203;
		air2.airInfo(); 
		AirPlane air3= new AirPlane();
		air3.name="KOREAN";
		air.number=304;
		air3.airInfo(); 	
		System.out.println("총누적대수는: "+air.number);
		
	}
}

System.out.println(result);

fc.CallSubSum(); //f3누르면 함수만든곳나옴.

result = fc.opSum(-1);

NoteBook nb = new NoteBook();
nb.color="d";
nb.setYear(2020);
int rs= nb.getYear();
System.out.println(rs);
	
	
    //a와 b중 큰값을 return max(500,300)일경우 500이 리턴되게.
    Scanner sc = new Scanner(System.in);

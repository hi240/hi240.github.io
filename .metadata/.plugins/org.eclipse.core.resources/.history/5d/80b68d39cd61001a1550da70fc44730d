import kr.or.bit.Emp;

public class Ex05_Array_Object {

	public static void main(String[] args) {
		//1. 사원 3명을 만드세요. (단 배열을 사용하세요)
		//1000사원번호, "홍길동"
		//2000,"김유신"
		//3000,"유관순"
		Emp[] emp = new Emp[3];
		for(int i=0;i<emp.length;i++) {
			emp[i]=new Emp();
			System.out.println(emp[i]);
		}
	}

}

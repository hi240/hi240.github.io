import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String dat=String.valueOf(100);
		System.out.println(dat);
        while(true)
        {	
    		int a=sc.nextInt();
    		int b=sc.nextInt();
        	if(a==0&&b==0) break;
               	System.out.println(a+b);
        }
	}
}

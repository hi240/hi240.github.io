
public class assignment {

	public static void main(String[] args) {
		//1번문제
		
		for(char i=65;i<=90;i++) {
			System.out.println(i);
		}
		
		//2번문제
		for (int j=1;j<=100;j++){
		      System.out.print(j+" ");
		    if(j==10||j==20||j==30||j==40||j==50||j==60||j==70||j==80||j==90)
		  System.out.println();
		}
		
        //조별과제 1번    -완료
		int sum1=0;
		for (int l=1;l<=20;l++) {
			if (l%2!=0&&l%3!=0) {
		sum1 +=l;
		}
		}
		System.out.printf("\n총합은 %d ",sum1);
		System.out.println();
        //조별과제 2번    -완료
		int sum2=0;
		for(int m=1;m<=6;m++){
		for(int n=1;n<=6;n++){
		 sum2=m+n;
		if(sum2==6)
		System.out.println("경우의 수는: "+m+" "+n);
		}
		}
		//조별과제 3번    -완료
		int a=((int) (Math.random() * 3) + 1);
		int d=((int) (Math.random() * 3) + 1);
		int b=0,c=0;	
		if (a==1) {  
			System.out.println("민수 : 가위");
		b=1;
		}
		else if (a==2){  
			System.out.println("민수 : 바위");
		b=2;
		} 
		else if (a==3){ 
			System.out.println("민수 : 보");
			b=3;
		} 
		if (d==1) {  
			System.out.println("영희 : 가위");
			c=1;
		}
		else if (d==2){  
			System.out.println("영희 : 바위");
			c=2;
		} 
		else if (d==3){ 
			System.out.println("영희 : 보");	
			c=3;
		} 
		if(b>c) {
			System.out.println("민수가 이겼다.");
		}
		if(c>b) {
			System.out.println("영희가 이겼다.");
		}
		if(b==c) {
				System.out.println("비겼다.");	
		}
	}
}


public class assignment {

	public static void main(String[] args) {
		//1번문제
		
		for(int i=65;i<=90;i++) {
			System.out.println(Character.toString((char)i));
		}
		
		
		//2번문제
		for (int j=1;j<=100;j++){
		      System.out.print(j+" ");
		    if(j==10||j==20||j==30||j==40||j==50||j==60||j==70||j==80||j==90)
		  System.out.println();
		}
        //조별과제 1번    -진행중
		int sum1=0;
		for (int l=1;l<=20;l++) {
			if (l%2!=0&&l%3!=0) {
		sum1 +=l;
		}
		}
		System.out.printf("\n 총합은 %d ",sum1);
        //조별과제 2번    -완료
		int sum2=0;
		for(int m=1;m<=6;m++){
		for(int n=1;n<=6;n++){
		 sum2=m+n;
		if(sum2==6)
		System.out.println("경우의 수는: "+m+" "+n);
		}
		}
	  
		
		//조별과제 3번
		System.out.println( ((int) (Math.random() * 10) + 1) );
		
		
		
	}
	
	
}

package kr.or.bit;

public class NoteBook {

	public String color;  //직접할당!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
	private String name; // private을 쓴 의도가 뭘까..?????????????????????????????????????????????????????????????????????
	private int year; //
	//직접 할당을 막고 간접할당을 통해서 자료를 보호하겠다........,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,,
	
	public void setYear(int y) {
		if(y<0) {
			year=1999;
		}else {
			year =y;
			
		}
		
		}
	
}

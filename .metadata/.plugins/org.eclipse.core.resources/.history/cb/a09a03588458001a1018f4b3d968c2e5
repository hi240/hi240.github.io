	private String name;
	private int number;
	private static int count;
	public void airInfo(String a,int b) {
		 name=a;
		 number=b;
     System.out.printf("항공기 넘버는 %s 번호는 %d \n",a,b);
     count++;
	}
	public int airCount() {
		return count;
	}
	
	
}

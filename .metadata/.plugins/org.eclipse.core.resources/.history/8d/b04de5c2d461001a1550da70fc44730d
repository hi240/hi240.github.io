
public class Ex06_Array2_basic {
public static void main (String[] args)
{
    //2차원 행과열
	//ex_ 영화관 예약 좌석배치'
	//추후과제..
	
	
	
	int[][] score = new int[3][2];
	System.out.println(score[0][0]);
	score [0][0]= 100;
	score [0][1]= 200;
	score [1][0]= 300;
	score [1][1]= 400;
	score [2][0]= 500;
	score [2][1]= 600;
	
	//행의 개수는 배열이름.length >score.length
	
			
	
	
	
	
 }
}
